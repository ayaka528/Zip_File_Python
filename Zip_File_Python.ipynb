   "outputs": [],
   "source": [
    "import glob\n",
    "import zipfile\n",
    " \n",
    "with zipfile.ZipFile('ssd_keras.zip', 'w') as z:\n",
    "    for f in glob.glob('ssd_keras/**', recursive=True):\n",
    "        # print(f)\n",
    "        z.write(f)"
   ]
  }
